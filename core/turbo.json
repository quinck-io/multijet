{
    "pipeline": {
        "typecheck": {},
        "build": {
            "dependsOn": ["^build", "typecheck"],
            "outputs": ["dist/**"]
        },
        "build:debug": {
            "dependsOn": ["^build:debug", "typecheck"]
        },
        "lint": {},
        "openapi:generate": {
            "cache": false,
            "filter": ["@libs/models"]
        },
        "test": {},
        "clean": {
            "cache": false
        }
    },
    "globalDependencies": ["tsconfig.base.json"]
}
